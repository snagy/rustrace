    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/rustrace.exe",
            "args": "160 100",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "symbolSearchPath":"${workspaceFolder}/target/debug",
            "environment": [],
            "externalConsole": true
        },
        {
            "name": "release",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/release/rustrace.exe",
            "args": "640 480",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true
        }
    ]
}